#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int a, b, c;
    cin>>a>>b>>c;
    setlocale(LC_ALL, " ");
    if (a*a+b*b==c*c)
    {
        cout<<"Треугольник является прямоугольным";
    }else
    {
        cout<<"Треугольник не является прямоугольным";
    }
    return 0;
}
