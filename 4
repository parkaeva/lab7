#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int a;
    cin>>a;
    setlocale(LC_ALL, " ");
    if ((a/100)>(a/10%10) && (a/10%10)>(a%10) || (a/100)<(a/10%10) && (a/10%10)<(a%10))
    {
        cout<<"Истина";
    }else
    {
        cout<<"Ложь";
    }
    return 0;
}
